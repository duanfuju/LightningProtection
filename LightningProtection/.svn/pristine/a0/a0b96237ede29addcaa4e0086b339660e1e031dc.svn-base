/** @Bind #dgshow.onDataRowDoubleClick */
!function(dgshow,tcShow,iframeInfo,ctinfo){
	var dataset =dgshow.get("dataSet");
	var row = dataset.getData("#");
	var _id= row.get("projectId");
	projectId=_id;
	iframeInfo.set("path","cn.com.oking.view.input.archiveSecond.normal.d?projectId="+projectId);
	tcShow.set('currentTab',ctinfo);
};
